{
  "name": "@qiwi/pijma-ssr",
  "packasso": [
    "@packasso/dev-ts",
    "@packasso/dev-license",
    "@packasso/build-ts-swc",
    "@packasso/style-eslint-prettier",
    "@packasso/release-zx-semrel"
  ],
  "dependencies": {
    "@emotion/server": "^11.10.0"
  },
  "devDependencies": {
    "@packasso/build-ts-swc": "0.16.1",
    "@packasso/dev-license": "0.9.0",
    "@packasso/dev-ts": "0.16.0",
    "@packasso/release-zx-semrel": "0.15.1",
    "@packasso/style-eslint-prettier": "0.15.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.37",
    "@swc/helpers": "0.4.14",
    "concurrently": "7.6.0",
    "css-loader": "6.7.1",
    "eslint": "8.35.0",
    "eslint-config-qiwi": "2.0.9",
    "fast-glob": "3.2.11",
    "file-loader": "6.2.0",
    "globby-cp": "1.1.3",
    "prettier": "2.8.4",
    "prettier-config-qiwi": "2.0.1",
    "react-docgen-typescript": "patch:react-docgen-typescript@npm%3A2.2.2#~/.yarn/patches/react-docgen-typescript-npm-2.2.2-afb9698a32.patch",
    "react-styleguidist": "12.0.0",
    "rimraf": "4.2.0",
    "style-loader": "3.3.1",
    "tsc-esm-fix": "2.20.12",
    "typescript": "4.9.5",
    "webpack": "5.75.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma/issues"
  },
  "homepage": "https://github.com/qiwi/pijma/#readme",
  "eslintConfig": {
    "rules": {
      "no-constant-condition": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "react/no-children-prop": "off",
      "react/no-find-dom-node": "off",
      "sonarjs/cognitive-complexity": "off",
      "sonarjs/no-duplicate-string": "off",
      "unicorn/no-negated-condition": "off",
      "unicorn/no-new-array": "off",
      "unicorn/prefer-add-event-listener": "off",
      "unicorn/prefer-dom-node-append": "off",
      "unicorn/prefer-dom-node-remove": "off",
      "unicorn/prefer-logical-operator-over-ternary": "off",
      "unicorn/prefer-number-properties": "off",
      "unicorn/prefer-optional-catch-binding": "off",
      "unicorn/prefer-regexp-test": "off",
      "unicorn/prefer-set-has": "off",
      "unicorn/prefer-spread": "off",
      "unicorn/prefer-string-slice": "off"
    },
    "extends": "eslint-config-qiwi"
  },
  "scripts": {
    "build": "packasso build",
    "lint": "packasso lint",
    "format": "packasso format"
  },
  "license": "MIT",
  "type": "module",
  "main": "./target/cjs/index.cjs",
  "module": "./target/esm/index.mjs",
  "types": "./target/dts/index.d.ts",
  "exports": {
    ".": {
      "require": "./target/cjs/index.cjs",
      "import": "./target/esm/index.mjs",
      "types": "./target/dts/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "target/cjs/**/*",
    "target/esm/**/*",
    "target/dts/**/*"
  ],
  "prettier": "prettier-config-qiwi",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "cmd": "yarn",
    "changelog": "changelog"
  }
}
